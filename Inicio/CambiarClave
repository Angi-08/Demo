<script>

    $(document).ready(function () {

        oForm.init();

    });


    var oForm = new function () {

        this.init = function () {

            $("#vErrores").hide();

        };


        this.validate = function () {


            var htmlContent = "";


            if ($('#ClaveActual').val().trim().length == 0) {

                htmlContent += "<li>Debe diligenciar la Clave Actual.</li>";

            }

            if ($('#ClaveNueva').val().trim().length == 0) {

                htmlContent += "<li>Debe diligenciar la Clave Nueva.</li>";

            }

            if ($('#ClaveConfirmar').val().trim().length == 0) {

                htmlContent += "<li>Debe Confirmar la Clave.</li>";

            }

            if ($('#ClaveNueva').val() != $('#ClaveConfirmar').val()) {

                htmlContent += "<li>Las Claves no son iguales.</li>";

            }

            if (htmlContent.length > 0) {

                this.showError(htmlContent);

                return false;
            }
            else {


                return true;
            }



        };



        this.showError = function (m) {

            $("#ErrorMessage").html(m);
            $("#vErrores").show();

        };



    }

</script>




